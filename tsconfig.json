{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "allowJs": false,
    "esModuleInterop": true,
    "lib": ["DOM", "ESNext"],
    "target": "ESNext",
    "noEmit": true,
   "plugins": [ 
    {
      
      "root": ["./src"],
      "extensions": [
        ".ios.ts",
        ".android.ts",
        ".ts",
        ".ios.tsx",
        ".android.tsx",
        ".tsx",
        ".jsx",
        ".js",
        ".json",
      ],
    },
  ]
  }
  
}
